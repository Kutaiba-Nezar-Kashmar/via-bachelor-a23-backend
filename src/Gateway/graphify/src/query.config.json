{
    "queries": [
        {
            "name": "events",
            "args": [
                {
                    "name": "from",
                    "for": "urlQueryParam",
                    "type": "String"
                },
                {
                    "name": "hostId",
                    "for": "urlQueryParam",
                    "type": "String"
                },
                {
                    "name": "includePrivateEvents",
                    "for": "urlQueryParam",
                    "type": "Boolean"
                }
            ],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 443,
                "endpoint": "/api/v1/events",
                "method": "GET"
            }
        },
        {
            "name": "allPublicEvents",
            "args": [],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 80,
                "endpoint": "/api/v1/events/allPublicEvents",
                "method": "GET"
            }
        },
        {
            "name": "event",
            "args": [
                {
                    "name": "eventId",
                    "type": "Int",
                    "for": "urlPath"
                }
            ],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "endpoint": "/api/v1/events/{eventId}",
                "port": 443,
                "method": "GET"
            }
        },
        {
            "name": "categories",
            "args": [],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 80,
                "endpoint": "/api/v1/categories/allCategories",
                "method": "GET"
            }
        },
        {
            "name": "keywords",
            "args": [],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 80,
                "endpoint": "/api/v1/keywords/allKeywords",
                "method": "GET"
            }
        },
        {
            "name": "reviewsByUser",
            "args": [
                {
                    "name": "userId",
                    "type": "String!",
                    "for": "urlQueryParam"
                }
            ],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 80,
                "endpoint": "/api/v1/reviews",
                "method": "GET"
            }
        },
        {
            "name": "finishedJoinedEvents",
            "args": [
                {
                    "name": "userId",
                    "type": "String!",
                    "for": "urlQueryParam"
                }
            ],
            "resolver": {
                "host": "http://eventmanagementservice-service",
                "port": 80,
                "endpoint": "/api/v1/events/finishedJoinedEvents",
                "method": "GET"
            }
        },
        {
            "name": "recommendations",
            "args": [
                {
                    "name": "userId",
                    "type": "String!",
                    "for": "urlQueryParam"
                },
                {
                    "name": "limit",
                    "type": "Float",
                    "for": "urlQueryParam"
                }
            ],
            "resolver": {
                "host": "http://recommendationservice-service",
                "port": 80,
                "endpoint": "/api/v1/recommendations",
                "method": "GET"
            }
        }
    ],
    "mutations": [
        {
            "name": "joinEvent",
            "args": [
                {
                    "name": "userId",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "eventId",
                    "type": "Int!",
                    "for": "body"
                }
            ],
            "resolver": {
                "host": "https://jsonplaceholder.typicode.com",
                "endpoint": "/api/v1/attendees",
                "port": 80,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "method": "POST"
            }
        },
        {
            "name": "createEvent",
            "args": [
                {
                    "name": "title",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "startDate",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "endDate",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "createdDate",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "lastUpdateDate",
                    "type": "String",
                    "for": "body"
                },
                {
                    "name": "isPrivate",
                    "type": "Boolean!",
                    "for": "body"
                },

                {
                    "name": "adultsOnly",
                    "type": "Boolean!",
                    "for": "body"
                },

                {
                    "name": "isPaid",
                    "type": "Boolean!",
                    "for": "body"
                },
                {
                    "name": "host",
                    "type": "userInput!",
                    "for": "body"
                },
                {
                    "name": "maxNumberOfAttendees",
                    "type": "Int!",
                    "for": "body"
                },
                {
                    "name": "url",
                    "type": "String",
                    "for": "body"
                },
                {
                    "name": "description",
                    "type": "String",
                    "for": "body"
                },
                {
                    "name": "location",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "city",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "geoLocation",
                    "type": "geoLocationInput!",
                    "for": "body"
                },
                {
                    "name": "category",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "keywords",
                    "type": "[String]",
                    "for": "body"
                }
            ],
            "resolver": {
                "host": "http://localhost:8080",
                "endpoint": "/api/v1/events/createEvent",
                "port": 8080,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "method": "POST"
            }
        },
        {
            "name": "createReview",
            "args": [
                {
                    "name": "rate",
                    "type": "Float!",
                    "for": "body"
                },
                {
                    "name": "reviewerId",
                    "type": "String!",
                    "for": "body"
                },
                {
                    "name": "eventId",
                    "type": "Int!",
                    "for": "body"
                },
                {
                    "name": "reviewDate",
                    "type": "String!",
                    "for": "body"
                }
            ],
            "resolver": {
                "host": "http://localhost:8080",
                "endpoint": "/api/v1/reviews/createReview",
                "port": 8080,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "method": "POST"
            }
        }
    ]
}
